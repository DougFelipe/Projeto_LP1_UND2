492 {\cf17 template}<{\cf17 class} E>\par
493 {\cf18 bool} Vetor<E>::equalsIgnoreCase({\cf17 const} std::string &str1, {\cf17 const} std::string &str2) \{\par
494   {\cf20 // criei essa fun\'E7\'E3o somente para ficar melhor de comparar as strings}\par
495   {\cf19 if} (str1.length() != str2.length()) \{\par
496     {\cf19 return} {\cf17 false};\par
497   \}\par
498 \par
499   {\cf19 for} ({\cf18 size_t} i = 0; i < str1.length(); i++) \{\par
500     {\cf19 if} (std::tolower(str1[i]) != std::tolower(str2[i])) \{\par
501       {\cf19 return} {\cf17 false};\par
502     \}\par
503   \}\par
504 \par
505   {\cf19 return} {\cf17 true};\par
506 \}\par
