360         list[pos] = list[i];\par
361         pos++;\par
362       \} {\cf19 else} {\cf19 if} (equalsIgnoreCase(valor, list[i].getEditora()) && op == 2)\{\par
363         list[pos] = list[i];\par
364         pos++;\par
365       \}\par
366     \}\par
367     qtd = pos;\par
