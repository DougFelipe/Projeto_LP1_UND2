374         {\cf19 for} ({\cf18 int} j = 0; j < qtd-1; j++) \{\par
375             {\cf19 if} (minuscula(list[j].getTitulo()) > minuscula(list[j+1].getTitulo())) \{\par
376                 {\cf20 // Troca livros[j] e livros[j+1]}\par
377                 E temp = list[j];\par
378                 list[j] = list[j+1];\par
379                 list[j+1] = temp;\par
380             \}\par
381         \}\par
382     \}\par
