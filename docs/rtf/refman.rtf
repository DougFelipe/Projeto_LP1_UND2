465   std::tm* ptm = std::localtime(&currentTime);\par
466 \par
467   std::string dataAtual = std::to_string(ptm->tm_mday) + {\cf22 "/"} + std::to_string( ptm->tm_mon + 1) + {\cf22 "/"} + std::to_string(ptm->tm_year + 1900);\par
468 \par
