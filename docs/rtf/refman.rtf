425 \par
426         {\cf19 if} (list[meio].getId() == chave)\par
427             {\cf19 return} meio;\par
428 \par
429         {\cf19 if} (list[meio].getId() < chave)\par
430             inicio = meio + 1;\par
431         {\cf19 else}\par
432             fim = meio - 1;\par
433     \}\par
434 \par
435     {\cf19 return} -1;\par
436 \}\par
