401 {\cf17 template}<{\cf17 class} E>\par
402 {\cf18 int} Vetor<E>::buscaBinariaRecursiva({\cf18 int} chave, {\cf18 int} inicio, {\cf18 int} fim) \{\par
403     {\cf19 if} (fim >= inicio) \{\par
404         {\cf18 int} meio = inicio + (fim - inicio) / 2;\par
405 \par
406         {\cf19 if} (list[meio].getId() == chave)\par
407             {\cf19 return} meio;\par
408 \par
409         {\cf19 if} (list[meio].getId() > chave)\par
410             {\cf19 return} buscaBinariaRecursiva(chave, inicio, meio - 1);\par
411 \par
412         {\cf19 return} buscaBinariaRecursiva(chave, meio + 1, fim);\par
413     \}\par
414 \par
415     {\cf19 return} -1;\par
416 \}\par
